{
  "name": "@irha-ha-addon/laskakit-data-feeder",
  "version": "0.0.1",
  "description": "Data Feeder for LaskaKit interactive map of Czech republic",
  "main": "dist/lib/server.js",
  "repository": "git@github.com:radoslavirha/ha-addon-laskakit-data-feeder.git",
  "author": "Radoslav Irha <radoslav.irha@gmail.com>",
  "private": true,
  "scripts": {
    "build": "yarn rmDist && tsc",
    "lint": "cross-env NODE_ENV=production eslint . --max-warnings=0",
    "lint:fix": "cross-env NODE_ENV=production eslint . --fix --max-warnings=0",
    "rmDist": "rm -rf dist",
    "start": "dotenv -e .env -- nodemon --transpile-only lib/server.ts",
    "start:prod": "cd dist/lib && node server.js",
    "test": "jest --coverage --verbose",
    "test:unit:ci": "jest --coverage --forceExit --logHeapUsage",
    "test:watch": "jest --watch --detectOpenHandles"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@hapi/hapi": "^21.3.2",
    "@hapi/inert": "^7.1.0",
    "@hapi/vision": "^7.0.1",
    "@hapipal/haute-couture": "^4.3.1",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.0",
    "hapi-swagger": "^16.0.1",
    "jimp": "^0.22.8",
    "joi": "^17.9.2",
    "join-images": "^1.1.5",
    "sharp": "^0.32.1",
    "terminal-image": "^2.0.0",
    "winston": "^3.8.2",
    "yn": "^4.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/hapi__vision": "^5.5.4",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^3.0.4",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">= 18.0.0",
    "npm": ">= 8.10.0",
    "yarn": ">= 1.22.18"
  }
}
