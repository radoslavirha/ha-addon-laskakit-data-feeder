{
  "name": "LaskaKit data feeder",
  "description": "Data Feeder for LaskaKit interactive map of Czech republic",
  "slug": "laskakitdatafeeder",
  "version": "0.2.4",
  "ingress": true,
  "ingress_port": 8000,
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
